# Environment Configuration for The Intersect API
# Copy this to .env and update with your actual values

# API Security
INTERSECT_API_KEY=your-super-secret-api-key-change-this-in-production

# Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=60

# CORS Origins (add your actual domains)
ALLOWED_ORIGINS=http://localhost:3000,https://tampertantrumlabs.com,https://www.tampertantrumlabs.com

# Ollama Configuration
OLLAMA_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1

# Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=info

# Production Settings
DEBUG=false
